<p id="notice"><%= notice %></p>

<h1>Related People</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @related_people.each do |related_person| %>
      <tr>
        <td><%= link_to 'Show', related_person %></td>
        <td><%= link_to 'Edit', edit_related_person_path(related_person) %></td>
        <td><%= link_to 'Destroy', related_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Related Person', new_related_person_path %>
